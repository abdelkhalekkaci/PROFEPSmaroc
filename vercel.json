json{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/public/$1.html"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/public/$1.css"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/$1.js"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/public/$1.png"
    },
    {
      "src": "/(.*)\\.jpg",
      "dest": "/public/$1.jpg"
    },
    {
      "src": "/(.*)\\.json",
      "dest": "/public/$1.json"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}